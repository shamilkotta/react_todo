  "bugs": {
    "url": "https://github.com/shamilkotta/react_todo/issues"
  },
  "homepage": "https://shamilkotta.github.io/react_todo",
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
